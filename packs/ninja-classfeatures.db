{"img":"systems/pf2e/icons/features/classes/incredible-sense.webp","name":"Incredible Senses","system":{"description":{"value":"<p>You notice things that are almost impossible for an ordinary person to detect. Your proficiency rank for Perception increases to legendary.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["hb_ninja"],"rarity":"common","custom":""},"rules":[{"key":"ActiveEffectLike","mode":"upgrade","path":"system.attributes.perception.rank","value":4}],"slug":"incredible-senses","schema":{"version":0.794,"lastMigration":null},"level":{"value":13},"featType":{"value":"classfeature"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":null},"prerequisites":{"value":[]},"location":null},"type":"feat","flags":{"core":{"sourceId":"Compendium.pf2e.classfeatures.nLwPMPLRne1HnL00"}},"effects":[],"ownership":{"default":0,"ChAfHXWjGQIj5GUP":3},"_stats":{"systemId":"pf2e","systemVersion":"4.2.6","coreVersion":"10.287","createdTime":1677103081573,"modifiedTime":1679919003388,"lastModifiedBy":"ChAfHXWjGQIj5GUP"},"folder":null,"sort":0,"_id":"QSPhBYNL1o1skWK6"}
